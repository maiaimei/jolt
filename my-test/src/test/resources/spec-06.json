[
  {
    "operation": "shift",
    "spec": {
      // match all elements
      "*": {
        // grab the key of the data at this level in the tree
        // and write it to month in the output
        "$": "month",
        // grab the value of the data at this level in the tree
        // and write it to count in the output
        "@": "count"
      }
    }
  }
]
