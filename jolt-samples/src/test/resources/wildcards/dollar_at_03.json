{
  "input": {
    "foo-a": "A",
    "foo-b": "B"
  },
  "spec": [
    {
      "operation": "shift",
      "spec": {
        "foo-*": {
          // '$' = '$0' = '$(0)' = '$(0,0)'
          "$(0,0)": "k1[]",
          "$(0,1)": "k2[]",
          "@": "value[]"
        }
      }
    }
  ],
  "expected": {
    "k1": [
      "foo-a",
      "foo-b"
    ],
    "k2": [
      "a",
      "b"
    ],
    "value": [
      "A",
      "B"
    ]
  }
}
